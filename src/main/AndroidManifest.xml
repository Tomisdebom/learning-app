<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eigenaar.softwarelearningapp">

    <!-- Give permission to access internet tto reach the web server -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SoftwareLearingApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SoftwareLearingApp.SignUp" />
        <activity android:name=".SoftwareLearingApp.ChapterSelect" />
        <activity android:name=".SoftwareLearingApp.lesson1" />
        <activity android:name=".SoftwareLearingApp.lesson2" />
        <activity android:name=".SoftwareLearingApp.lesson3" />
        <activity android:name=".SoftwareLearingApp.lessons" />
        <activity android:name=".SoftwareLearingApp.lesson1T1" />
        <activity android:name=".SoftwareLearingApp.lesson2T1" />
        <activity android:name=".SoftwareLearingApp.lesson3T1" />
        <activity android:name=".SoftwareLearingApp.lesson1T2" />
        <activity android:name=".SoftwareLearingApp.lesson2T2" />
        <activity android:name=".SoftwareLearingApp.lesson3T2" />
        <activity android:name=".SoftwareLearingApp.question1" />
        <activity android:name=".SoftwareLearingApp.question2" />
        <activity android:name=".SoftwareLearingApp.question3" />
        <activity android:name=".SoftwareLearingApp.Correct"></activity>
    </application>

</manifest>